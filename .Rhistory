eq_location_clean <- function(data) {
data <- data %>%
dplyr::mutate_(LOCATION_NAME = ~LOCATION_NAME %>%
stringr::str_replace(paste0(COUNTRY, ":"), "") %>%
stringr::str_trim("both") %>%
stringr::str_to_title())
data
}
